{
	"name": "ArtiFalse",
	"version": "1.0",
	"description": "Disable ArtiMedia ads",
	"browser_action": {
		"default_icon": {
			"16": "assets/icon_16.png",
			"32": "assets/icon_32.png",
			"48": "assets/icon_48.png",
			"128": "assets/icon_128.png"
		},
		"default_popup": "./src/popup.html"
	},
	"icons": {
		"16": "assets/icon_16.png",
		"32": "assets/icon_32.png",
		"48": "assets/icon_48.png",
		"128": "assets/icon_128.png"
	},
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"manifest_version": 2
}